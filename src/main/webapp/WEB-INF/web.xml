<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>Freelancer</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <listener>
    <listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>
  </listener>
  <listener>
    <listener-class>org.springframework.web.context.request.RequestContextListener</listener-class>
  </listener>

  <servlet>
    <description></description>
    <display-name>OAuthRequestToken</display-name>
    <servlet-name>OAuthRequestToken</servlet-name>
    <servlet-class>com.panic.servlets.OAuthRequestToken</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>OAuthRequestToken</servlet-name>
    <url-pattern>/servlet/OAuthRequestToken</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>OAuthCallback</display-name>
    <servlet-name>OAuthCallback</servlet-name>
    <servlet-class>com.panic.servlets.OAuthCallback</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>OAuthCallback</servlet-name>
    <url-pattern>/servlet/OAuthCallback</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>APIProxy</display-name>
    <servlet-name>APIProxy</servlet-name>
    <servlet-class>com.panic.servlets.APIProxy</servlet-class>
  </servlet>
    <servlet-mapping>
    <servlet-name>APIProxy</servlet-name>
    <url-pattern>/proxy/*</url-pattern>
  </servlet-mapping>
</web-app>